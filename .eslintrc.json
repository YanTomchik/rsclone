{
    "env": {
        "browser": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "import/extensions": [
            0
        ],
        "no-use-before-define": [
            0
        ],
        "no-plusplus": [
            0
        ],
        "no-mixed-operators": [
            0
        ],
        "max-len": [
            0
        ]
    }
}